<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="443.0" prefWidth="701.0" styleClass="background" stylesheets="@../style/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.japanredsun.Controller.Home">
   <children>
      <VBox prefHeight="457.0" prefWidth="702.0">
         <children>
            <Label alignment="CENTER" prefHeight="137.0" prefWidth="692.0" text="TOEIC APP">
               <font>
                  <Font name="Tahoma Bold" size="50.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" prefHeight="246.0" prefWidth="692.0" spacing="20.0">
               <children>
                  <Button id="btnStart" fx:id="btnStart" mnemonicParsing="false" onAction="#switchtest" prefHeight="65.0" prefWidth="235.0" styleClass="btnGreen" stylesheets="@../style/style.css" text="Start Test" textFill="WHITE">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Button>
                  <Button id="btnManage" mnemonicParsing="false" onAction="#switchadmin" prefHeight="65.0" prefWidth="235.0" styleClass="btnGreen" text="Manage Question" textFill="WHITE">
                     <font>
                        <Font size="25.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="lberror" alignment="CENTER" prefHeight="40.0" prefWidth="692.0" textFill="RED">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <GridPane prefHeight="95.0" prefWidth="702.0">
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="631.0" minWidth="10.0" prefWidth="504.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="631.0" minWidth="10.0" prefWidth="96.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="639.0" minWidth="10.0" prefWidth="92.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="29.0" minHeight="10.0" prefHeight="27.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="34.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="ID:" GridPane.columnIndex="1">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label text="Password:" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label text="Role:" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Text id="txtid" fx:id="txtid" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="2">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Text>
                  <Text id="txtpass" fx:id="txtpass" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Text>
                  <Text id="txtrole" fx:id="txtrole" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="2" GridPane.rowIndex="2">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Text>
               </children>
            </GridPane>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="35.0" prefWidth="692.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#switchuser" styleClass="btnGreen" text="Manage User" textFill="WHITE">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#signout" styleClass="btnGreen" text="Sign Out" textFill="WHITE">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </VBox>

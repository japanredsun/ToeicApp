   <TextField fx:id="txtUserName" layoutX="216.0" layoutY="128.0" promptText="Username" />
   <PasswordField fx:id="txtPassword" layoutX="217.0" layoutY="175.0" promptText="Password" />
   <Label fx:id="lbError" layoutX="263.0" layoutY="300.0" textFill="RED">
      <font>
         <Font name="Tahoma" size="19.0" />
      </font>
   </Label>
</AnchorPane>
